#!/bin/sh
MONITORLIST=$(xrandr --listmonitors)
getmonitor () {
    MONITOR=`echo "$MONITORLIST" | grep "$1:" | cut -d ' ' -f3`
    MONITOR="${MONITOR//[+\*]/}"
    if [ ! -z $MONITOR ]
    then
        echo $MONITOR
        exit
    fi
}

i=$1
while [ $i -ge 0 ]
do
    getmonitor "$i"
    i=$(expr $i - 1)
done