#!/bin/sh
if [[ -z $DOTFILES ]]
then
    echo "\$DOTFILES not set"
    exit 1
fi

echo "== These should all be links to $DOTFILES/configs/* =="
ls -l "~/.gitconfig"
ls -l "~/.gtkrc-2.0"
ls -l "~/.profile"
ls -l "~/.xinitrc"
ls -l "~/.config/Code - OSS/User/settings.json"
ls -l "~/.config/compton.conf"
ls -l "~/.config/dunst/dunstrc"
ls -l "~/.config/gtk-3.0/gtk.css"
ls -l "~/.config/gtk-3.0/settings.ini"
ls -l "~/.config/i3/config"
ls -l "~/.config/polybar/config"
ls -l "~/.config/redshift.conf"
ls -l "~/.config/rofi/config"
ls -l "~/.config/rofi/rofi-my-dark-theme.rasi"
ls -l "~/.config/wal/templates/dunstrc"
ls -l "~/.config/wal/templates/rofi-my-dark-theme.rasi"
ls -l "~/.config/wal/templates/xresources-00-base"
ls -l "~/.config/Xresources.d/00-base"