{
  "explorer.confirmDelete": false,
  "editor.renderWhitespace": "all",
  "editor.formatOnSave": true,
  "editor.fontFamily": "Fira Code, monospace, Font Awesome",
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "window.zoomLevel": 0,
  "editor.tabSize": 2,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "node_modules": true
  },
  "workbench.colorCustomizations": {
    "focusBorder": "#fadbb0",
    "foreground": "#f5efde",
    "widget.shadow": "#aba79b",
    "selection.background": "#a341f4",
    "descriptionForeground": "#fadbb0",
    "errorForeground": "#a341f4",
    "input.background": "#362749",
    "input.foreground": "#f5efde",
    "input.placeholderForeground": "#aba79b",
    "inputOption.activeBorder": "#fadbb0",
    "inputValidation.errorBackground": "#a341f4",
    "inputValidation.errorBorder": "#a341f4",
    "inputValidation.infoBackground": "#fadbb0",
    "inputValidation.infoBorder": "#fadbb0",
    "inputValidation.warningBackground": "#f886ac",
    "inputValidation.warningBorder": "#f886ac",
    "list.invalidItemForeground": "#a341f4",
    "list.errorForeground": "#a341f4",
    "list.warningForeground": "#f886ac",
    "terminal.background": "#362749",
    "terminal.foreground": "#f5efde",
    "terminalCursor.background": "#362749",
    "terminalCursor.foreground": "#f5efde",
    "terminal.ansiBlack": "#362749",
    "terminal.ansiRed": "#a341f4",
    "terminal.ansiGreen": "#e948f5",
    "terminal.ansiYellow": "#f886ac",
    "terminal.ansiBlue": "#fadbb0",
    "terminal.ansiMagenta": "#b672f7",
    "terminal.ansiCyan": "#e68ff9",
    "terminal.ansiWhite": "#f5efde",
    "terminal.ansiBrightBlack": "#aba79b",
    "terminal.ansiBrightRed": "#a341f4",
    "terminal.ansiBrightGreen": "#e948f5",
    "terminal.ansiBrightYellow": "#f886ac",
    "terminal.ansiBrightBlue": "#fadbb0",
    "terminal.ansiBrightMagenta": "#b672f7",
    "terminal.ansiBrightCyan": "#e68ff9",
    "terminal.ansiBrightWhite": "#f5efde",
    "activityBar.background": "#362749",
    "activityBar.foreground": "#f5efde",
    "sideBarSectionHeader.background": "#362749",
    "sideBar.background": "#362749",
    "sideBar.foreground": "#f5efde",
    "statusBar.background": "#b672f7",
    "statusBar.foreground": "#f5efde",
    "statusBar.debuggingBackground": "#e948f5",
    "statusBar.noFolderBackground": "#e948f5",
    "tab.activeBackground": "#362749",
    "tab.activeForeground": "#f5efde",
    "editor.background": "#362749",
    "editorOverviewRuler.modifiedForeground": "#f886ac",
    "editorOverviewRuler.addedForeground": "#e948f5",
    "editorOverviewRuler.deletedForeground": "#a341f4",
    "editorOverviewRuler.warningForeground": "#f886ac",
    "editorOverviewRuler.infoForeground": "#fadbb0",
    "editorOverviewRuler.errorForeground": "#a341f4",
    "editorGutter.background": "#362749",
    "editorGutter.modifiedBackground": "#f886ac",
    "editorGutter.addedBackground": "#e948f5",
    "editorGutter.deletedBackground": "#a341f4",
    "editorError.foreground": "#a341f4",
    "editorWarning.foreground": "#f886ac",
    "editorInfo.foreground": "#fadbb0",
    "editorHint.foreground": "#e948f5",
    "gitDecoration.modifiedResourceForeground": "#f886ac",
    "gitDecoration.deletedResourceForeground": "#a341f4",
    "gitDecoration.untrackedResourceForeground": "#e948f5"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.colorTheme": "Monokai",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}